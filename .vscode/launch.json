{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: mkv4cafr --input 'medias/test01.mkv' --edit-in-place",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/mkv4cafr.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": "--input \"medias/test01.mkv\" --edit-in-place"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}",
            "cwd": "${fileDirname}",
        }
    ]
}
